<% if game.game_image.present? %>
  <h3>Image Gallery</h3>
  <ul>
    <% proxy_url = image_proxy_url(game_image_key(game), width: 1200, height: 900, fit: "inside", format: "webp") %>
    <li><%= image_tag(proxy_url, alt: game.game_name, loading: "lazy", decoding: "async") if proxy_url %></li>
  </ul>
<% end %>
